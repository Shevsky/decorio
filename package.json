{
  "name": "decorio",
  "version": "1.0.3",
  "description": "First-class ECMAScript decorators for caching, binding, and concurrency patterns",
  "keywords": [
    "decorators",
    "ecmascript",
    "es",
    "stage-3",
    "cache",
    "caching",
    "memoize",
    "debounce",
    "throttle",
    "singleflight",
    "mutex",
    "latest",
    "once",
    "cached",
    "bound",
    "typescript",
    "ts",
    "metadata",
    "utility"
  ],
  "license": "MIT",
  "repository": "git@github.com:Shevsky/decorio.git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "postbuild": "esbuild './dist/**/*.js' --minify --allow-overwrite --format=esm --log-level=silent --outdir=dist",
    "test": "vitest --run"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.3",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@types/node": "22.15.19",
    "esbuild": "0.25.5",
    "prettier": "3.5.3",
    "semantic-release": "24.2.5",
    "tsc-alias": "1.8.16",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.3"
  }
}
